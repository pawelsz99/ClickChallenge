<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.HomeFragment">

    <Button
        android:id="@+id/home_quit_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="home_quit_button"
        tools:layout_editor_absoluteX="122dp"
        tools:layout_editor_absoluteY="391dp" />

    <TextView
        android:id="@+id/home_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/racing_sans_one"
        android:text="@string/app_name"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="99dp" />

    <Button
        android:id="@+id/home_play_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="home_play_button"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="181dp" />

    <Button
        android:id="@+id/home_more_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="home_more_button"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="286dp" />
</androidx.constraintlayout.widget.ConstraintLayout>
